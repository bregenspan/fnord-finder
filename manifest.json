{
  "name": "Fnord Finder",
  "version": "1.2",
  "description": "Finds the fnords in New York Times articles.",
  "homepage_url": "http://en.wikipedia.org/wiki/Fnord",
  "background_page": "background.html",
  "page_action" :
  {
    "default_icon" : "icon19.png",
    "default_title" : "Fnords found!"
  },
  "permissions" : [
    "tabs",
    "http://*.nytimes.com/"
  ],
  "icons" : {
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "content_security_policy": "script-src 'self'"
}

